{
  "name": "bubblebot",
  "version": "0.0.16",
  "description": "A slackbot the manages cloud-hosted infrastructure",
  "main": "lib/index.js",
  "scripts": {
    "prepublish": "coffee -o lib -c src && node node_modules/eslint/bin/eslint.js lib",
    "test": "node node_modules/eslint/bin/eslint.js lib && node lib/bbserver"
  },
  "bin": {
    "bubblebot": "bin/bubblebot.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bubblegroup/bubblebot.git"
  },
  "keywords": [
    "slack",
    "deployment"
  ],
  "author": "Bubble Group, Inc <contact@bubble.is>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bubblegroup/bubblebot/issues"
  },
  "homepage": "https://github.com/bubblegroup/bubblebot",
  "dependencies": {
    "@slack/client": "^2.3.0",
    "aws-sdk": "^2.4.2",
    "escape-html": "^1.0.3",
    "eslint": "^2.7.0",
    "express": "^4.14.0",
    "express-session": "^1.14.0",
    "fibers": "^1.0.10",
    "json-stable-stringify": "^1.0.1",
    "librato-metrics": "0.0.7",
    "moment": "^2.13.0",
    "moment-timezone": "^0.5.4",
    "passport": "^0.3.2",
    "passport-slack": "0.0.6",
    "pg": "^4.5.5",
    "prompt": "^1.0.0",
    "request": "^2.71.0",
    "scp2": "^0.2.2",
    "shrinkpack": "^0.10.2",
    "ssh2": "^0.5.0",
    "strip-json-comments": "^2.0.1",
    "tmp": "0.0.28"
  },
  "eslintConfig": {
    "rules": {
      "no-undef": 2
    },
    "env": {
      "browser": true,
      "node": true,
      "jquery": true
    },
    "globals": {
      "mixpanel": true,
      "braintree": true,
      "Handsontable": true
    }
  }
}
